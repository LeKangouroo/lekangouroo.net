    "whatwg-fetch": "3.0.0",
    "yargs": "12.0.2"
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  },
  "config": {
    "scripts_bin": "gulp --gulpfile taskrunner/gulpfile.babel.js"
  },
  "scripts": {
    "build:pre-production": "$npm_package_config_scripts_bin build --env=pre-production --mode=distributable",
    "build:production": "$npm_package_config_scripts_bin build --env=production --mode=distributable",
    "clean": "$npm_package_config_scripts_bin clean",
    "dev": "$npm_package_config_scripts_bin dev",
    "fclean": "npm run clean -- --dist",
    "release": "standard-version",
    "zip": "$npm_package_config_scripts_bin zip"
  }
}
